    @Scripts.Render("~/bundles/validationApp-js")
    @Scripts.Render("~/bundles/app")

    @*@Styles.Render("~/bundles/ui-layout-js")*@
    @*@Styles.Render("~/bundles/rough-js")*@

    <script src="~/Scripts/FlowChart.js"></script>
    <script src="~/Scripts/ui-layout.js"></script>
    <script src="~/Scripts/ngFlowchart.js"></script>
    <script src="~/Scripts/angular-drag-and-drop-lists.min.js"></script>
    <script src="~/Scripts/Draggable.js"></script>


   @*<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular.js"></script>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-2.5.0.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.1/angular-animate.js"></script>
